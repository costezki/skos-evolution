###  http://costezki.ro/interval-ontology#during

:during rdf:type owl:ObjectProperty ,
                 owl:TransitiveProperty ;
        
        rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#after

:after rdf:type owl:ObjectProperty ,
                owl:TransitiveProperty ;
       
       owl:inverseOf :before ;
       
       rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#before

:before rdf:type owl:ObjectProperty ,
                 owl:TransitiveProperty ;
        
        rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#equals

:equals rdf:type owl:ObjectProperty ,
                 owl:ReflexiveProperty ,
                 owl:SymmetricProperty ,
                 owl:TransitiveProperty ;
        
        rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#finishedBy

:finishedBy rdf:type owl:AsymmetricProperty ,
                     owl:ObjectProperty ;
            
            owl:inverseOf :finishes ;
            
            rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#finishes

:finishes rdf:type owl:AsymmetricProperty ,
                   owl:ObjectProperty ;
          
          rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#intervalRelation

:intervalRelation rdf:type owl:ObjectProperty ;
                  
                  rdfs:comment "relation that relates two segments in a particular way"^^rdfs:Literal ;
                  
                  rdfs:range :Interval ;
                  
                  rdfs:domain :Interval .



###  http://www.isfla.org/interval-ontology#meets

:meets rdf:type owl:AsymmetricProperty ,
                owl:ObjectProperty ;
       
       rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#metBy

:metBy rdf:type owl:AsymmetricProperty ,
                owl:ObjectProperty ;
       
       rdfs:subPropertyOf :intervalRelation ;
       
       owl:inverseOf :meets .



###  http://www.isfla.org/interval-ontology#overlaps

:overlaps rdf:type owl:ObjectProperty ,
                   owl:ReflexiveProperty ,
                   owl:SymmetricProperty ;
          
          rdfs:subPropertyOf :intervalRelation .



###  http://www.isfla.org/interval-ontology#startedBy

:startedBy rdf:type owl:AsymmetricProperty ,
                    owl:ObjectProperty ;
           
           rdfs:subPropertyOf :intervalRelation ;
           
           owl:inverseOf :starts .



###  http://www.isfla.org/interval-ontology#starts

:starts rdf:type owl:AsymmetricProperty ,
                 owl:ObjectProperty ;
        
        rdfs:subPropertyOf :intervalRelation .





